{
  "name": "go-timer",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:release": "react-native run-android --mode=release",
    "ios": "react-native run-ios --simulator='iPhone 14'",
    "release:android": "npm run cp:node_modules && cd android && ./gradlew bundleRelease && cd .. && open android/app/build/outputs/bundle/release",
    "lint": "eslint .",
    "start": "react-native start",
    "start:reset-cache": "react-native start --reset-cache",
    "test": "jest",
    "postinstall": "yarn run cp:node_modules",
    "pod:install": "npx pod-install ios",
    "generate:assets": "node ./scripts/generate-assets-import",
    "gradlew:clean": "cd android && ./gradlew clean && cd ..",
    "xcrun:simctl-list-devices": "xcrun simctl list devices",
    "open:xc": "open ios/GoTimer.xcworkspace",
    "adb:devices": "adb devices",
    "avd:list": "emulator -list-avds",
    "avd:turn-on-device": "emulator -avd Pixel_6_API_31",
    "eslint:fix": "eslint src --fix",
    "eslint:check": "eslint src",
    "cp:android": "cp -rf ../__android__/* ./android",
    "cp:node_modules": "cp -rf ./__node_modules__/* ./node_modules"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "4.0.1",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-navigation/native": "6.1.4",
    "@react-navigation/native-stack": "6.9.10",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "i18next": "22.4.10",
    "react": "18.2.0",
    "react-i18next": "12.1.5",
    "react-native": "0.71.3",
    "react-native-google-mobile-ads": "9.1.2",
    "react-native-localize": "2.2.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "3.20.0",
    "react-native-sound": "0.11.2",
    "react-native-tts": "4.1.0",
    "styled-components": "5.3.6",
    "styled-system": "5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "5.1.26",
    "@types/styled-components-react-native": "5.2.1",
    "@types/styled-system": "5.1.16",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
